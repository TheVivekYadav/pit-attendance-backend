    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "rollNo": {
      "type": "string"
    },
    "student_academic_years": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::student-academic-year.student-academic-year",
